    <Declaration><![CDATA[PROGRAM Elbow
VAR
	t AT %Q*: REAL := 0;
	tip : REAL := 0.01;
	tarrad AT %I* : REAL;
	rad AT %I* : REAL;
	m1v AT %I* : REAL;
	m2v AT %I* : REAL;
	err: REAL;
	test AT %Q* : INT;
	a0 AT %Q* : INT;
	a1 AT %Q* : INT;
	a2 AT %Q* : INT;
	a3 AT %Q* : INT;
	a4 AT %Q* : INT;
	a5 AT %Q* : INT;
    N_save: INT := 1;
    ERRORCODE: INT := 0;
	
	v1 AT %I* : INT;
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[t := t + tip;
rad;
tarrad;
err := tarrad - rad;
