IF  DATA[N_save].g=DATA[N_save].h THEN
		DATA[N_save].p1:=tarrad;
		DATA[N_save].p2:=rad;
		DATA[N_save].p3:=err;
		DATA[N_save].p4:=m1v;
		DATA[N_save].p5:=m2v;
		DATA[N_save].g:=NOT DATA[N_save].g ;
		N_save:=N_save+1;
		IF  N_save>200 THEN
			N_save:=1;
		END_IF
ELSE
		ERRORCODE:=1;
END_IF

